<Page
    x:Class="LykkeWalletApp.Views.Regisrations.RegFullName"
    xmlns:nm="using:LykkeWalletApp.Controls"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel Background="White" Padding="10">
        <nm:RegistrationHeader Header="REGISTER" Description="Complete your profile"/>
        <nm:WizardControl Height="50px"/>
        <TextBox x:Name="EdtFullName" Header="Enter your Fullname" Margin="0 50 0 0" KeyUp="EdtFullName_OnKeyUp"/>
        <Button x:Name="BtnSbmt" HorizontalAlignment="Stretch" IsEnabled="False" Click="BtnSbmt_OnClick">SUBMIT</Button>
    </StackPanel>
    
    
    
</Page>
